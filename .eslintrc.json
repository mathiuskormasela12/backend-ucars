{
	"env": {
		"es2021": true,
		"node": true
	},
	"extends": [
		"airbnb-base"
	],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": [
		"@typescript-eslint"
	],
	"rules": {
		"no-tabs": "off",
		"no-mixed-spaces-and-tabs": "off",
		"import/no-unresolved": "off",
		"import/extensions": "off",
		"import/prefer-default-export": "off",
		"no-console": "off"
	}
}
